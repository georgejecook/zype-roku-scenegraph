<?xml version="1.0" encoding="utf-8" ?>

<component name="VideoNode" extends="ContentNode">
  <interface>
    <field id="inFavorites" type="boolean" />
    <field id="onAir" type="boolean" />
    <field id="contentId" type="string" />
    <field id="mediaType" type="string" />
    <field id="posterThumbnail" type="string" />
    <field id="usePoster" type="boolean" />

    <field id="subscriptionRequired" type="boolean" />
    <field id="purchaseRequired" type="boolean" />
    <field id="rentalRequired" type="boolean" />
    <field id="passRequired" type="boolean" />
    <field id="registrationRequired" type="boolean" />

    <!-- Associated in-store product (if tvod) -->
    <field id="storeProduct" type="assocarray" />

    <!-- for pagination -->
    <field id="isPaginator" type="boolean" />
    <field id="nextPage" type="integer" />
  </interface>
</component>